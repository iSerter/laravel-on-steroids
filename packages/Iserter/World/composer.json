{
    "name": "iserter/laravel-world",
    "description": "Laravel package with world data, models, migrations, and possibly more in the future.",
    "license": "license",
    "authors": [
        {
            "name": "ilyas serter",
            "email": "ilyasserter@gmail.com",
            "homepage": "https://www.ilyasserter.com"
        }
    ],
    "homepage": "https://github.com/iserter/laravel-world",
    "keywords": [
        "Laravel",
        "World",
        "Countries",
        "Cities",
        "States",
        "Laravel World",
        "World data"
    ],
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "illuminate/support": "~5|~6"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Iserter\\World\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Iserter\\World\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Iserter\\World\\WorldServiceProvider"
            ],
            "aliases": {
                "World": "Iserter\\World\\Facades\\World"
            }
        }
    }
}
